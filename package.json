{
  "name": "tribe-29-checkmk-datasource",
  "version": "4.0.3",
  "description": "Datasource for checkmk",
  "scripts": {
    "build": "webpack -c ./webpack.config.ts --env production",
    "build:cloud": "webpack -c ./webpack.config.ts --env production",
    "build:dev": "webpack -c ./webpack.config.ts --env development",
    "dev": "webpack -w -c ./webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint --fix",
    "pretty": "prettier -c '{**/*,*}.{js,ts,jsx,tsx,json,yml}'",
    "pretty:fix": "prettier -w '{**/*,*}.{js,ts,jsx,tsx,json,yml}'",
    "server": "docker-compose up --build",
    "start": "npm run watch",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "e2e": "playwright test",
    "e2e:ci": "playwright test",
    "e2e:debug": "playwright test --debug",
    "e2e:gui": "playwright test --ui",
    "e2e:show-trace": "playwright show-trace"
  },
  "author": "Checkmk",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/plugin-e2e": "^1.19.4",
    "@grafana/tsconfig": "^2.0.1",
    "@playwright/test": "^1.56.1",
    "@swc/core": "^1.13.5",
    "@swc/helpers": "^0.5.17",
    "@swc/jest": "^0.2.39",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.9.1",
    "@types/react-router-dom": "^5.3.3",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "dotenv": "^17.2.3",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-webpack-plugin": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "glob": "^11.0.3",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.6.2",
    "react-intersection-observer": "^9.16.0",
    "react-select-event": "^5.5.1",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.93.2",
    "sass-loader": "16.0.6",
    "style-loader": "4.0.0",
    "swc-loader": "^0.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@grafana/data": "^12.0.2",
    "@grafana/runtime": "^11.6.2",
    "@grafana/schema": "^11.6.2",
    "@grafana/ui": "^11.6.2",
    "process": "^0.11.10",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tslib": "2.8.1"
  },
  "packageManager": "npm@11.6.2"
}
